{
  "prettier.enable": true,
  "eslint.enable": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "vitest.enable": true,
  "vitest.commandLine": "npm test --",
  "testing.automaticallyOpenPeekView": "never"
}
